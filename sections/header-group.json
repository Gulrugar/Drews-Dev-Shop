{
  "name": "t:sections.header.name",
  "type": "header",
  "sections": {
    "announcement-bar": {
      "type": "announcement-bar",
      "blocks": {
        "announcement-bar-0": {
          "type": "announcement",
          "settings": {
            "text": "Welcome to Drew's Development Store",
            "text_alignment": "center",
            "color_scheme": "accent-1",
            "link": ""
          }
        }
      },
      "block_order": ["announcement-bar-0"],
      "settings": {}
    },
    "header": {
      "type": "custom-header",
      "custom_css": [
        ".header__heading-logo {transition: transform .2s ease;}",
        ".header__heading-logo:hover {transform: scale(1.1)}"
      ],
      "settings": {
        "logo_position": "middle-left",
        "menu": "main-menu",
        "menu_type_desktop": "dropdown",
        "sticky_header_type": "on-scroll-up",
        "show_line_separator": true,
        "color_scheme": "accent-2",
        "enable_country_selector": false,
        "enable_language_selector": false,
        "mobile_logo_position": "center",
        "margin_bottom": 0,
        "padding_top": 20,
        "padding_bottom": 20
      }
    }
  },
  "order": ["announcement-bar", "header"]
}
